class Solution {
public:
    int reverse(int x) {
        stack<int> n;
        double rev=0;
        long tens=1;
        while(x!=0)
        {
            n.push(x%10);
            x=x/10;
        }
        while(!n.empty())
        {
           rev=n.top()*tens+rev;
           tens=tens*10;
            n.pop();
        }
        if(rev>pow(2,31) || rev<(pow(-2,31)-1))
            return 0;
        else return rev;
    }
};
